{
  "rules": [
    {
      "name": "Work_by_Organization",
      "description": "An organization typically produces a work.",
      "premises": [
        {
          "predicate": "Entity",
          "args": [
            "work_type"
          ]
        },
        {
          "predicate": "Organization",
          "args": [
            "org_type"
          ]
        }
      ],
      "conclusion": {
        "predicate": "Work",
        "args": [
          "work_type",
          "medium"
        ]
      }
    },
    {
      "name": "Work_on_Date",
      "description": "A work is produced on a date.",
      "premises": [
        {
          "predicate": "Entity",
          "args": [
            "work_type"
          ]
        },
        {
          "predicate": "Work",
          "args": [
            "work_type",
            "medium"
          ]
        }
      ],
      "conclusion": {
        "predicate": "Date",
        "args": [
          "date_type"
        ]
      }
    },
    {
      "name": "Work_by_Person",
      "description": "A person typically produces a work.",
      "premises": [
        {
          "predicate": "Entity",
          "args": [
            "person_type"
          ]
        },
        {
          "predicate": "Person",
          "args": [
            "person_type",
            "nationality"
          ]
        }
      ],
      "conclusion": {
        "predicate": "Work",
        "args": [
          "work_type",
          "medium"
        ]
      }
    }
  ]
}