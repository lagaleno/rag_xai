{
  "rules": [
    {
      "name": "Authorship inheritance",
      "description": "An author's works are of a certain type if they are an author and their work is of that type.",
      "premises": [
        {
          "predicate": "AUTHOR",
          "args": [
            "entity",
            "work"
          ]
        },
        {
          "predicate": "TYPE",
          "args": [
            "entity",
            "type"
          ]
        }
      ],
      "conclusion": {
        "predicate": "TYPE",
        "args": [
          "work",
          "type"
        ]
      }
    }
  ]
}